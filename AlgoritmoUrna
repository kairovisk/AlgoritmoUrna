#include<stdio.h>

int main() {
    struct enderemes {
        char estadomes[100],cidademes[100],bairromes[30],ruames[30],numcasames[10],cepmes[10],compmes;
     };
      struct {
        int codmes,idademes,cpfmes,titulomes,telmes, turnomes;
        char nomemes[50],generomes,especialmes[3],qualmes, vinculomes,beneficiomes,historicomes;
         struct enderemes endmes;
      } mesario[5];
    struct {
        int codcom, CPF, ipurna, ippc;
        char nomecom, acessocom;
    } comite;
    struct {
        int codpart,quantpart;
        char nomepart, membrospart;
    } partido[X];
    struct {
        int numcand, codpart_cand;
        char nomecand, nomepart_cand, tipocand;
    } candidato[X -   DETALHE];
    struct {
        int numtit_eleit, cnpj, rg, codzona, codsecao;
        char nome, profiss;
    } eleitor[X -   DETALHE];
    struct enderezona {
        char estadozona[100],cidadezona[100],bairrozona[30],ruazona[30],numzona[10],cepzona[10]
    };
    struct {
        int codzona_vot;
        char nomezona, secoes;
        struct enderezona endzona;
    } localvot;
}



